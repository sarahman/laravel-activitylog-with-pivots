{
    "name": "sarahman/laravel-activitylog-with-pivots",
    "description": "Activity Log package deals with all kinds of Eloquent events based logging as well as pivot models events.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.0",
        "fico7489/laravel-pivot": "^3.0",
        "spatie/laravel-activitylog": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Sarahman\\Database\\Support\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
